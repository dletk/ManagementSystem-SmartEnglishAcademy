<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="800.0" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="600.0">
         <children>
            <ImageView fitHeight="124.0" fitWidth="411.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../res/posterSEA.png" />
               </image>
            </ImageView>
            <VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <children>
                  <Label text="THỜI GIAN LÀM VIỆC" textFill="#1b78f7">
                     <font>
                        <Font name="Times New Roman Bold" size="26.0" />
                     </font>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets bottom="30.0" top="30.0" />
                     </VBox.margin>
                  </Label>
                  <TableView fx:id="tableHours" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <columns>
                      <TableColumn fx:id="dateColumn" editable="false" prefWidth="75.0" text="NGÀY" />
                      <TableColumn fx:id="startingTimeColumn" prefWidth="75.0" text="BẮT ĐẦU" />
                        <TableColumn fx:id="endingTimeColumn" prefWidth="75.0" text="KẾT THÚC" />
                        <TableColumn fx:id="lengthColumn" prefWidth="75.0" text="THỜI LƯỢNG (giờ)" />
                    </columns>
                     <VBox.margin>
                        <Insets bottom="10.0" left="20.0" right="20.0" />
                     </VBox.margin>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</HBox>
